{
  "manifest_version": 3,
  "name": "IPO Account Manager",
  "version": "1.0.0",
  "description": "Manage multiple IPO accounts, track allotment, and calculate profits.",
  "permissions": [
    "storage"
  ],
  "action": {
    "default_popup": "index.html",
    "default_title": "IPO Manager"
  },
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}